{
  "name": "n8n-nodes-botamin",
  "version": "0.2.0",
  "description": "Botamin custom nodes",
  "license": "UNLICENSED",
  "keywords": [
    "n8n-community-node-package",
    "kafka",
    "botamin"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && cp -R nodes dist && cp dist/nodes/BotaminKafkaTrigger/kafka.svg dist/BotaminKafkaTrigger/ && cp dist/nodes/BotaminKafkaOutput/kafka.svg dist/BotaminKafkaOutput/",
    "dev": "npm run build -- --watch",
    "lint": "eslint . --ext .ts"
  },
  "n8n": {
    "nodes": [
      "dist/BotaminKafkaTrigger/BotaminKafkaTrigger.node.js",
      "dist/BotaminKafkaOutput/BotaminKafkaOutput.node.js"
    ],
    "credentials": []
  },
  "devDependencies": {
    "@types/node": "^18.19.68",
    "eslint": "^9.18.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "kafkajs": "^2.2.4",
    "n8n-workflow": "^1.118.1"
  }
}
